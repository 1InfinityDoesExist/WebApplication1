<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="RegisterBean" table="Registration" schema="sys">
		<id name="SerialNo" type="int" column="SerialNo">
			<generator class="native"></generator>
		</id>
		<property name="userName" type="string" column="UserName"></property>
		<property name="userPassword" type="string" column="Password"></property>
	</class>

</hibernate-mapping>